        
{% extends '@SonataAdmin/CRUD/base_list_field.html.twig' %}

{% block field %}
    <div>
        {% if object.mainTeacher is not empty %}
            <a href="{{path('mark_table', {'section_id': object.id}) }}" target="_blank">
                <img src="{{ asset('images/mark_table3.jpg') }}" alt="{{ object.name }}" style="width: 85px; height: 65px;"/>
            </a>
        {% else %}
            <a href="#">
                <img src="{{ asset('images/mark_table_false.jpeg') }}" alt="{{ object.name }}" style="width: 85px; height: 65px;"/>
            </a>
        {% endif %}
        <br/>
    </div>
{% endblock %}